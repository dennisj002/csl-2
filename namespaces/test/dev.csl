int factoriali ( byte REG n | REG res )
{
    res = 1;
    while ( n >= 2 )
    {
#if 0
        res *= n-- ; 
#elif 0
        res *= (n--) ; 
#elif 0
        res = res * n-- ;
#else
        res = res * (n--) ;
#endif
    }
    return res;
} prefix
