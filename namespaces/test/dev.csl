int
fibPTC2? ( REG n | REG fn, REG fn1, REG fn2 )
{
    for ( fn = 0, fn1 = 0, fn2 = 1 ; n ; n -- ) 
    {   
        fn1 = fn2 ;
        fn2 = fn ;
        fn = fn1 + fn2 ; 
    }
    return fn ;
}
